    "@szhsin/react-menu": "^4.0.3",
    "@webcontainer/api": "^1.1.5",
    "antd": "^5.6.4",
    "jszip": "^3.10.1",
    "monaco-jsx-syntax-highlight": "^1.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-resizable-panels": "^0.0.54",
    "react-router": "^6.14.1",
    "react-router-dom": "^6.14.1",
    "react-spring": "^9.7.2",
    "react-use-measure": "^2.1.1",
    "uuid": "^9.0.0",
