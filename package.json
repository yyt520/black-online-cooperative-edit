    "@laconic/react-script": "^1.2.0",
    "@types/node": "^20.5.1",
    "@types/prettier": "^2.7.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "@types/react-resizable": "^3.0.4",
    "@types/uuid": "^9.0.2",
    "@types/wicg-file-system-access": "^2020.9.6",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "commitizen": "^4.3.0",
