    "sass": "^1.63.6",
    "standard-version": "^9.5.0",
    "typescript": "^4.9.5"
  },
  "homepage": "http://black.github.io/black-online-cooperative-edit",
  "browserslist": [
    "last 2 version",
    "> 1%",
    "not dead"
  ],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
